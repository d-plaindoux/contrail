require NetClient, NetServer;

adminServer = ....

payLoad = function(){
	delta "...";
}

clientFlow = function(uri){
	flow = payLoad() <> serialize() <> coerce(Event);
	reverse(flow) <> client(uri);   
}

serverFlow = function(uri){
	flow = function(client){ 
		client <> [ Jaxb() | PayLoad() <> Serialize() ] <> coerce(Event) 
	};
	server(uri,flow) <> adminServer;	
}


