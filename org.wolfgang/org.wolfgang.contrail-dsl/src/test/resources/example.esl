import org.wolfgang.contrail.connection.net.NetClient;
import org.wolfgang.contrail.connection.net.NetServer;

def clientFlow = fun uri -> client uri <>  payload () <> object () <> coerce Event;   

def serverFlow = fun uri -> {
	def flow = fun client -> client <> switch [ jaxb() | payload() <> object() ] <> coerce Event;			
	server uri flow  <> adminServer;	
}

def print = fun message -> (System.println message).echo (); 


