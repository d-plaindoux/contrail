<?xml version='1.0'?>
<ecosystem>
	<import>
		org.wolfgang.contrail.connection.net.NetClient
	</import>

	<define name="Coercion">
		<function>
			<var>type</var>
			<ref>coerce</ref>
		</function>
	</define>

	<define name='Client'>
		<function>
			<var>uri</var>
			<ref>client</ref>
		</function>
	</define>

	<define name='Main'>
		<function>
			<var>bind</var>
			<ref>bind</ref>
			<appy>
				<ref>reverse</ref>
				<flow>
					<ref>payload</ref>
					<ref>object</ref>
				</flow>
			</appy>
			<apply>
				<ref>Coercion</ref>
				<atom>java.lang.String</atom>
			</apply>
			<apply>
				<ref>Client</ref>
				<atom>tcp://localhost:6666</atom>
			</apply>
		</function>
	</define>
</ecosystem>
