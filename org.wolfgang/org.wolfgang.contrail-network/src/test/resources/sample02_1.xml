<?xml version='1.0'?>
<ecosystem>
	<import library='org.wolfgang.contrail.connection.net.NetClient' />

	<define name="Coerce">
		<function>
			<var>type</var>
			<ref>coerce</ref>
		</function>
	</define>

	<define name='Client'>
		<function>
			<var>uri</var>
			<ref>client</ref>
		</function>
	</define>

	<define name='Main'>
		<function>
			<var>bind</var>
			<apply>
				<ref>Client</ref>
				<atom>tcp://localhost:6666</atom>
			</apply>
			<ref>payload</ref>
			<ref>object</ref>
			<apply>
				<ref>Coerce</ref>
				<atom>java.lang.String</atom>
			</apply>
			<ref>bind</ref>
		</function>
	</define>
</ecosystem>
