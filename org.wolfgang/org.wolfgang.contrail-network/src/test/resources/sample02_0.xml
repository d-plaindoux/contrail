<?xml version='1.0'?>
<ecosystem>
	<import>
		org.wolfgang.contrail.connection.net.NetClient
	</import>

	<import as='ClientComponent'>
		org.wolfgang.contrail.component.connection.ClientComponent
	</import>

	<define name='Client'>
		<function>
			<var>uri</var>
			<ref>ClientComponent</ref>
		</function>
	</define>

	<define name='Main'>
		<function>
			<var>uri</var>
			<var>client</var>
			<apply>
				<ref>Client</ref>
				<ref>uri</ref>
			</apply>
			<ref>client</ref>
		</function>
	</define>
</ecosystem>
