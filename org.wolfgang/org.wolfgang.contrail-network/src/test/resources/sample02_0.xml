<?xml version='1.0'?>
<ecosystem>
	<import>
		org.wolfgang.contrail.connection.net.NetClient
	</import>

	<import as='ClientComponent'>
		org.wolfgang.contrail.component.connection.ClientComponent
	</import>

	<define name='Client'>
		<function>
			<var>uri</var>
			<ref>ClientComponent</ref>
		</function>
	</define>

	<binder name='Main' in="byte[]" out="byte[]">
		<apply>
			<ref>Client</ref>
			<atom>tcp://localhost:6666</atom>
		</apply>
	</binder>
</ecosystem>
